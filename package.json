{
  "name": "carolinarazor",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "packageManager": "pnpm@10.17.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-ensvv NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "ci": "payload migrate && pnpm build && pnpm start",
    "generate:migration": "payload migrate:create",
    "job:newsletter": "tsx ./scripts/generate-newsletter.ts",
    "job:broadcast": "tsx ./scripts/generate-broadcast.ts",
    "job:clearratelimiter": "tsx ./scripts/clear-ratelimit-entries.ts"
  },
  "dependencies": {
    "@heroui/navbar": "^2.2.27",
    "@heroui/system": "^2.4.25",
    "@heroui/theme": "^2.4.25",
    "@payloadcms/admin-bar": "3.68.5",
    "@payloadcms/db-postgres": "3.68.5",
    "@payloadcms/email-resend": "3.68.5",
    "@payloadcms/live-preview-react": "3.68.5",
    "@payloadcms/next": "3.68.5",
    "@payloadcms/plugin-form-builder": "3.68.5",
    "@payloadcms/plugin-nested-docs": "3.68.5",
    "@payloadcms/plugin-redirects": "3.68.5",
    "@payloadcms/plugin-search": "3.68.5",
    "@payloadcms/plugin-seo": "3.68.5",
    "@payloadcms/richtext-lexical": "3.68.5",
    "@payloadcms/ui": "3.68.5",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-email/components": "^1.0.6",
    "@react-email/render": "^2.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "16.4.7",
    "framer-motion": "^12.26.2",
    "geist": "^1.5.1",
    "graphql": "^16.12.0",
    "lucide-react": "^0.378.0",
    "next": "15.5.11",
    "next-sitemap": "^4.2.3",
    "payload": "3.68.5",
    "prism-react-renderer": "^2.4.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.45.4",
    "react-icons": "^5.5.0",
    "resend": "^6.9.1",
    "sharp": "0.34.2",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.21.0",
    "typescript": "5.7.3",
    "validator": "^13.15.26"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "1.56.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.5.4",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "@types/validator": "^13.15.10",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.23",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.5.11",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ],
    "ignoredBuiltDependencies": [
      "@heroui/shared-utils"
    ]
  }
}
